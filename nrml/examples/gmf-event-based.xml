<?xml version='1.0' encoding='utf-8'?>
<!-- GMF model for Event-Based calculator results.
     Note that the structure varies slightly in the Scenario calculator
     GMF results. -->
<nrml xmlns="http://openquake.org/xmlns/nrml/0.4">
    <gmfCollection sourceModelTreePath="b1|b2|b4" gsimTreePath="b1|b7">
        <gmfSet investigationTime="50.0">
            <gmf IMT="SA" saPeriod="0.025" saDamping="5.0">
                <node iml="0.2" lon="0.0" lat="0.0" />
                <node iml="1.4" lon="1.0" lat="0.0" />
                <node iml="0.6" lon="0.0" lat="1.0" />
            </gmf>
            <gmf IMT="PGA">
                <node iml="0.2" lon="0.0" lat="0.0" />
                <node iml="1.4" lon="1.0" lat="0.0" />
                <node iml="0.6" lon="0.0" lat="1.0" />
            </gmf>
            <gmf IMT="PGV">
                <node iml="0.2" lon="0.0" lat="0.0" />
                <node iml="1.4" lon="1.0" lat="0.0" />
                <node iml="0.6" lon="0.0" lat="1.0" />
            </gmf>
        </gmfSet>
        <gmfSet investigationTime="40.0">
            <gmf IMT="SA" saPeriod="0.025" saDamping="5.0">
                <node iml="0.2" lon="0.0" lat="0.0" />
                <node iml="1.4" lon="1.0" lat="0.0" />
                <node iml="0.6" lon="0.0" lat="1.0" />
            </gmf>
            <gmf IMT="PGA">
                <node iml="0.2" lon="0.0" lat="0.0" />
                <node iml="1.4" lon="1.0" lat="0.0" />
                <node iml="0.6" lon="0.0" lat="1.0" />
            </gmf>
            <gmf IMT="PGV">
                <node iml="0.2" lon="0.0" lat="0.0" />
                <node iml="1.4" lon="1.0" lat="0.0" />
                <node iml="0.6" lon="0.0" lat="1.0" />
            </gmf>
        </gmfSet>
    </gmfCollection>
</nrml>
